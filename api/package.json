{
  "name": "api",
  "description": "",
  "version": "0.0.0",
  "homepage": "",
  "main": "server",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "iva2k",
    "email": "iva2k@yahoo.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "server",
    "test": "test/"
  },
  "engines": {
    "node": "^8.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run eslint && npm run coverage",
    "coverage": "npm run clean && cross-env NODE_ENV=test nyc npm run mocha && nyc report --reporter=html",
    "eslint": "eslint server/. test/. --config .eslintrc.json",
    "start"        : "npm run start:dev",
    "start:dev"    : "cross-env NODE_ENV=dev     node server/",
    "start:prod"   : "cross-env NODE_ENV=prod    node server/",
    "start:staging": "cross-env NODE_ENV=staging node server/",
    "clean": "shx rm -rf test/data",
    "mocha": "mocha test/ --recursive --exit"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/*.test.js"
    ],
    "watermarks": {
      "lines": [70, 95],
      "functions": [70, 95],
      "branches": [70, 95],
      "statements": [70, 95]
    }
  },
  "dependencies": {
    "@feathersjs/configuration": "^1.0.2",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.3",
    "@feathersjs/feathers": "^3.1.7",
    "@feathersjs/socketio": "^3.2.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "feathers-nedb": "^3.0.1",
    "feathers-seeder": "^1.0.10",
    "helmet": "^3.12.1",
    "nedb": "^1.8.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^4.8.0",
    "feathers-memory": "^2.1.3",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "shx": "^0.3.0"
  }
}
